      "targets": [
        {
          "datasource": {
            "type": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_metrics AS (\n  SELECT \n    project_key,\n    metric_key,\n    metric_value,\n    recorded_at,\n    ROW_NUMBER() OVER (PARTITION BY project_key, metric_key ORDER BY recorded_at DESC) as rn\n  FROM sonarqube_metrics\n)\nSELECT \n  p.project_name,\n  MAX(CASE WHEN lm.metric_key = 'bugs' THEN lm.metric_value END) as bugs,\n  MAX(CASE WHEN lm.metric_key = 'vulnerabilities' THEN lm.metric_value END) as vulnerabilities,\n  MAX(CASE WHEN lm.metric_key = 'code_smells' THEN lm.metric_value END) as code_smells,\n  MAX(CASE WHEN lm.metric_key = 'coverage' THEN lm.metric_value END) as coverage,\n  MAX(CASE WHEN lm.metric_key = 'ncloc' THEN lm.metric_value END) as lines_of_code\nFROM sonarqube_projects p\nLEFT JOIN latest_metrics lm ON p.project_key = lm.project_key AND lm.rn = 1\nGROUP BY p.project_name\nORDER BY bugs DESC NULLS LAST\nLIMIT 50;",
          "refId": "A"
        }
      ],
      "title": "Top 50 Projects by Bugs",
      "type": "table"
    }
