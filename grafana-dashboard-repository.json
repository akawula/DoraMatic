        {
          "datasource": {
            "type": "postgres"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  generate_series(\n    DATE_TRUNC('day', $__timeFrom()::timestamp),\n    DATE_TRUNC('day', $__timeTo()::timestamp),\n    '1 day'::interval\n  ) as time,\n  (\n    SELECT AVG(daily_count)::numeric(10,1)\n    FROM (\n      SELECT COUNT(*) as daily_count\n      FROM prs pr\n      WHERE pr.repository_name = '$repository'\n        AND pr.created_at >= $__timeFrom()::timestamp\n        AND pr.created_at <= $__timeTo()::timestamp\n      GROUP BY DATE_TRUNC('day', pr.created_at)\n    ) daily_counts\n  ) as \"Average PRs Created\"",
          "refId": "C"
