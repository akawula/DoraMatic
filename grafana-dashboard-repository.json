        {
          "datasource": {
            "type": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  pr.title as \"PR Title\",\n  pr.author as \"Author\",\n  pr.state as \"State\",\n  COALESCE(r.language, 'Unknown') as \"Language\",\n  pr.additions as \"Additions\",\n  pr.deletions as \"Deletions\",\n  (pr.additions + pr.deletions) as \"Total Changes\",\n  CASE\n    WHEN (pr.additions + pr.deletions) <= 100 THEN 'Small'\n    WHEN (pr.additions + pr.deletions) <= 500 THEN 'Medium'\n    ELSE 'Large'\n  END as \"PR Size\",\n  pr.created_at as \"Created At\",\n  pr.merged_at as \"Merged At\",\n  CASE\n    WHEN pr.merged_at IS NOT NULL THEN\n      CASE\n        WHEN EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) < 60 THEN\n          ROUND(EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at))) || ' seconds'\n        WHEN EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) < 3600 THEN\n          FLOOR(EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) / 60) || ' minutes ' ||\n          ROUND(EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) % 60) || ' seconds'\n        WHEN EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) < 86400 THEN\n          FLOOR(EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) / 3600) || ' hours ' ||\n          FLOOR((EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) % 3600) / 60) || ' minutes'\n        ELSE\n          FLOOR(EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) / 86400) || ' days ' ||\n          FLOOR((EXTRACT(EPOCH FROM (pr.merged_at - pr.created_at)) % 86400) / 3600) || ' hours'\n      END\n    ELSE NULL\n  END as \"Lead Time\"\nFROM prs pr\nLEFT JOIN repositories r ON pr.repository_name = r.slug AND pr.repository_owner = r.org\nWHERE pr.repository_name = '$repository'\n  AND pr.created_at >= $__timeFrom()::timestamp\n  AND pr.created_at <= $__timeTo()::timestamp\nORDER BY pr.created_at DESC\nLIMIT 100",
          "refId": "A"
        }
      ],
      "title": "Repository Pull Requests (Selected Period)",
      "type": "table"
